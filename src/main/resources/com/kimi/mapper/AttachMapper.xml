<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.kimi.mapper.AttachMapper">
  
	<select id="getAttachList" resultType="com.kimi.model.attachDTO">
	
		select * from st_attach where st_id = #{st_id}
	
	</select>
	
	<select id="getMenuAttachList" resultType="com.kimi.model.attachDTO">
	
		select * from mn_attach where mn_id = #{mn_id}
	
	</select>

  </mapper>
 